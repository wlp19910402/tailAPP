<view class="content"><view class="uni-padding-wrap"><view class="page-section swiper"><view class="page-section-spacing"><swiper class="swiper" indicator-dots="{{indicatorDots}}" autoplay="{{autoplay}}" interval="{{interval}}" duration="{{duration}}"><swiper-item><image src="../static/images/banner-1.jpg"></image></swiper-item><swiper-item><image src="../static/images/banner-2.jpg"></image></swiper-item><swiper-item><image src="../static/images/banner-3.jpg"></image></swiper-item></swiper></view></view></view><view class="query-ticket-box"><view class="query-ticket-state"><view data-event-opts="{{[['tap',[['e0',['$event']]]]]}}" class="{{['state-name',endorse?'state-name-gray':'']}}" bindtap="__e">{{startTrainName}}</view><image class="state-icon" src="../static/images/state-icon.png"></image><view data-event-opts="{{[['tap',[['e1',['$event']]]]]}}" class="{{['state-name',endorse?'state-name-gray':'']}}" bindtap="__e">{{endTrainName}}</view></view><view class="query-ticket-state"><view class="time-name">出发日期</view><block wx:if="{{!endorse}}"><view data-event-opts="{{[['tap',[['chooseDate',['$event']]]]]}}" class="go-date" bindtap="__e">{{godate}}</view></block><block wx:if="{{endorse}}"><view data-event-opts="{{[['tap',[['chooseDateEndorse',['$event']]]]]}}" class="go-date" bindtap="__e">{{godate}}</view></block><view class="time-week">{{dateweek}}</view></view><view class="query-ticket-state"><checkbox-group class="checkbox-box"><label>只看高铁/动车<checkbox style="transform:scale(0.6);" value="cb" checked="true" color="#007AFF"></checkbox></label><label>学生票<checkbox style="transform:scale(0.6);" value="cb" color="#007AFF"></checkbox></label></checkbox-group></view><block wx:if="{{!endorse}}"><navigator url="{{'../ticketModule/queryTicket?date='+godate+'&week='+dateweek+'&startTrain='+startTrainName+'&endTrain='+endTrainName}}"><button class="tail-btn" type="primary">查询车票</button></navigator></block><block wx:if="{{endorse}}"><view url="{{'orderQueryTicket?date='+godate+'&week='+dateweek+'&startTrain='+startTrainName+'&endTrain='+endTrainName}}" data-event-opts="{{[['tap',[['endorseNavigator',['$event']]]]]}}" bindtap="__e"><button class="tail-btn" type="primary">查询车票</button></view></block></view><view class="ad-image-box"><image src="../static/images/banner-1.jpg"></image><image src="../static/images/banner-2.jpg"></image><image src="../static/images/banner-3.jpg"></image></view></view>