<view><view class="ticket-box"><view class="flex-box"><view class="tail-detail"><view class="tail-train-type">{{orderData[0].startTrainState}}</view><view class="trainName"><view>{{orderData[0].startTrainName}}</view><view class="train-time">{{orderData[0].departureTime}}</view></view></view></view><view class="flex-box"><view class="tail-name">{{orderData[0].tailName}}</view><image class="arrival-icon" src="../../../static/images/ticket-icon.jpg"></image><view class="arrival-time">{{orderData[0].timeLength}}</view></view><view class="flex-box"><view class="tail-detail"><view class="tail-train-type">{{orderData[0].endTrainState}}</view><view class="trainName"><view>{{orderData[0].endTrainName}}</view><view class="train-time">{{orderData[0].arrivalTime}}</view></view></view></view></view><view class="seat-box"><block wx:for="{{orderData[0].seat}}" wx:for-item="item" wx:for-index="eq" wx:key="eq"><view data-event-opts="{{[['tap',[['seatSelect',['$0','$1'],[[['orderData.__$n0.seat','',eq,'number']],[['orderData.__$n0.seat','',eq,'seatId']]]]]]]}}" class="{{['seat-list',item.seatId===(!whentherSeat?defaultSeatId:dSeatId)?'seat-list-active':'']}}" bindtap="__e"><view class="{{['seat-name',item.number>0?'':'gray-color']}}">{{item.name}}</view><view class="{{['seat-num',item.number>0?'':'gray-color']}}"><block wx:if="{{item.number>0&&item.number<50}}"><text>{{item.number+"张"}}</text></block><block wx:if="{{item.number>=50}}"><text>有</text></block><block wx:if="{{item.number===0}}"><text>无</text></block></view><view class="{{['seat-price',item.number>0?'':'gray-color']}}">{{"¥"+item.price}}</view></view></block></view></view>