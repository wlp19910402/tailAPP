<view><block wx:for="{{tailData}}" wx:for-item="item" wx:for-index="index" wx:key="index"><view class="uni-list-cell" hover-class="uni-list-cell-hover" data-newsid="{{item.trainId}}"><navigator url="{{!endorse?'confirmOrder?newsid='+item.trainId+'&date='+godate+'&week='+dateweek:'confirmOrderEndorse?'+'&date='+godate+'&week='+dateweek}}"><view class="uni-media-list"><view class="uni-media-list-body"><view class="tail-name">{{item.tailName}}</view><view class="flex-box"><view class="tail-detail flex-box"><view class="tail-train-type">{{item.startTrainState}}</view><view class="trainName">{{item.startTrainName}}</view></view><view class="train-time">{{item.departureTime}}</view></view><view class="flex-box"><image class="arrival-icon" src="../static/images/arrival-icon.jpg"></image><view class="arrival-time">{{item.timeLength}}</view></view><view class="flex-box"><view class="tail-detail flex-box"><view class="tail-train-type">{{item.endTrainState}}</view><view class="trainName">{{item.endTrainName}}</view></view><view class="train-time">{{item.arrivalTime}}</view></view><uni-icon class="train-list-btn" vue-id="{{'1-'+index}}" type="arrowright" size="20" bind:__l="__l"></uni-icon></view><view class="seat-box"><block wx:for="{{item.seat}}" wx:for-item="ite" wx:for-index="eq" wx:key="eq"><view class="seat-list"><text class="{{['seat-name',ite.number>0?'':'gray-color']}}">{{ite.name+":"}}</text><text class="seat-num"><block wx:if="{{ite.number>0&&ite.number<50}}"><text>{{ite.number+"张"}}</text></block><block wx:if="{{ite.number>=50}}"><text class="green-color">有</text></block><block wx:if="{{ite.number===0}}"><text class="gray-color">无</text></block></text><block wx:if="{{false}}"><text class="seat-price">{{"¥"+ite.price}}</text></block></view></block></view></view></navigator></view></block></view>