
.content {
	text-align: center;
}
.order-title {
	width: 100%;
	-webkit-box-sizing: border-box;
	        box-sizing: border-box;
	padding-left: 30rpx;
	line-height: 90rpx;
	text-align: left;
	font-size: 36rpx;
	border-bottom: 1rpx solid #eeeeee;
}
.order-fun-box {
	width: 100%;
	-webkit-box-sizing: border-box;
	        box-sizing: border-box;
	padding: 30rpx 30rpx 10rpx;
	overflow: hidden;
}
.order-fun-list {
	width: 25%;
	text-align: center;
	float: left;
}
.navigator {
	width: 120rpx;
	padding: 10rpx 0;
	margin: 0 auto;
}
.navigator image {
	width: 60rpx;
	height: 60rpx;
}
.navigator view {
	line-height: 40rpx;
	font-size: 30rpx;
}
.ad-image-box {
	width:750rpx;
	margin-bottom: 20rpx;
}
.ad-image-box image {
	-webkit-box-sizing: border-box;
	        box-sizing: border-box;
	
	height: 200rpx;
	width: 100%;
	margin: 20rpx 0 0;
}

