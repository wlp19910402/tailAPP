<view class="page"><ticket-date vue-id="1" godate="{{godate}}" dateweek="{{dateweek}}" data-event-opts="{{[['^updateGodate',[['__set_sync',['$0','godate','$event'],['']]]],['^updateDateweek',[['__set_sync',['$0','dateweek','$event'],['']]]]]}}" bind:updateGodate="__e" bind:updateDateweek="__e" bind:__l="__l"></ticket-date><comfirm-order vue-id="2" defaultSeatId="{{defaultSeatId}}" orderData="{{orderData}}" seat="{{seat}}" data-event-opts="{{[['^updateDefaultSeatId',[['__set_sync',['$0','defaultSeatId','$event'],['']]]],['^updateDefaultSeatId',[['__set_sync',['$0','defaultSeatId','$event'],['']]]],['^updateOrderData',[['__set_sync',['$0','orderData','$event'],['']]]],['^updateOrderData',[['__set_sync',['$0','orderData','$event'],['']]]],['^updateSeat',[['__set_sync',['$0','seat','$event'],['']]]]]}}" bind:updateDefaultSeatId="__e" bind:updateOrderData="__e" bind:updateSeat="__e" bind:__l="__l"></comfirm-order><view class="user"><block wx:for="{{userData}}" wx:for-item="item" wx:for-index="index" wx:key="index"><view class="user-list"><view class="floatLeft"><text class="user-text">{{item.name}}</text><text class="user-text">{{item.certificateId}}</text></view><view class="floatRight"><text class="user-text">{{item.adult}}</text><text>二等座</text><checkbox style="transform:scale(0.5);" backgroundColor="#666666" color="#666666"></checkbox></view></view></block></view><navigator url="{{'waitPay?date='+godate+'&week='+dateweek}}"><button class="tail-btn" type="primary">提交订单</button></navigator></view>